<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#f9f9f9"/>
  
  <!-- Title -->
  <text x="400" y="40" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle">Quantum Neural Networks</text>
  <text x="400" y="70" font-family="Arial" font-size="16" text-anchor="middle">Comparison with Classical Neural Networks</text>
  
  <!-- Main sections -->
  <rect x="50" y="100" width="340" height="380" rx="10" ry="10" fill="#ffcccc" fill-opacity="0.2" stroke="#cc0000" stroke-width="2"/>
  <text x="220" y="130" font-family="Arial" font-size="18" font-weight="bold" text-anchor="middle" fill="#cc0000">Classical Neural Network</text>
  
  <rect x="410" y="100" width="340" height="380" rx="10" ry="10" fill="#ccccff" fill-opacity="0.2" stroke="#0066cc" stroke-width="2"/>
  <text x="580" y="130" font-family="Arial" font-size="18" font-weight="bold" text-anchor="middle" fill="#0066cc">Quantum Neural Network</text>
  
  <!-- Classical Neural Network visualization -->
  <!-- Input Layer -->
  <circle cx="120" cy="200" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="120" cy="250" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="120" cy="300" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="120" cy="350" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  
  <!-- Hidden Layer -->
  <circle cx="220" cy="175" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="220" cy="225" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="220" cy="275" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="220" cy="325" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="220" cy="375" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  
  <!-- Output Layer -->
  <circle cx="320" cy="225" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="320" cy="275" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  <circle cx="320" cy="325" r="15" fill="#ffcccc" stroke="#cc0000" stroke-width="1.5"/>
  
  <!-- Connections between layers -->
  <!-- Input to Hidden -->
  <line x1="135" y1="200" x2="205" y2="175" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="200" x2="205" y2="225" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="200" x2="205" y2="275" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="200" x2="205" y2="325" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="200" x2="205" y2="375" stroke="#cc0000" stroke-width="1"/>
  
  <line x1="135" y1="250" x2="205" y2="175" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="250" x2="205" y2="225" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="250" x2="205" y2="275" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="250" x2="205" y2="325" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="250" x2="205" y2="375" stroke="#cc0000" stroke-width="1"/>
  
  <line x1="135" y1="300" x2="205" y2="175" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="300" x2="205" y2="225" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="300" x2="205" y2="275" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="300" x2="205" y2="325" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="300" x2="205" y2="375" stroke="#cc0000" stroke-width="1"/>
  
  <line x1="135" y1="350" x2="205" y2="175" stroke="#cc0000" stroke-width="1"/>
  <line x1="135" y1="350" x2="205" y2="225" stroke="#cc0000" stroke-width="1"/>
