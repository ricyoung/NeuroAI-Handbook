  <line x1="235" y1="175" x2="305" y2="275" stroke="#cc0000" stroke-width="1"/>
  <line x1="235" y1="175" x2="305" y2="325" stroke="#cc0000" stroke-width="1"/>
  
  <line x1="235" y1="225" x2="305" y2="225" stroke="#cc0000" stroke-width="1"/>
  <line x1="235" y1="225" x2="305" y2="275" stroke="#cc0000" stroke-width="1"/>
  <line x1="235" y1="225" x2="305" y2="325" stroke="#cc0000" stroke-width="1"/>
  
  <line x1="235" y1="275" x2="305" y2="225" stroke="#cc0000" stroke-width="1"/>
