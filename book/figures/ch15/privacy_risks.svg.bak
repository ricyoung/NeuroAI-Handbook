<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#f9f9f9"/>
  
  <!-- Title -->
  <text x="400" y="40" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle">Neural Data Privacy Risks</text>
  <text x="400" y="70" font-family="Arial" font-size="16" text-anchor="middle">Potential Vulnerabilities in NeuroAI Systems</text>
  
  <!-- Brain in the center -->
  <ellipse cx="400" cy="300" rx="150" ry="120" fill="#ffcccc" stroke="#cc0000" stroke-width="3"/>
  
  <!-- Brain details -->
  <path d="M 340 240 C 320 220, 320 280, 340 260 Z" fill="#cc0000" stroke="#cc0000" stroke-width="1"/>
  <path d="M 460 240 C 480 220, 480 280, 460 260 Z" fill="#cc0000" stroke="#cc0000" stroke-width="1"/>
  <path d="M 370 230 C 360 200, 380 200, 370 230 Z" fill="#cc0000" stroke="#cc0000" stroke-width="1"/>
  <path d="M 430 230 C 420 200, 440 200, 430 230 Z" fill="#cc0000" stroke="#cc0000" stroke-width="1"/>
  <path d="M 400 220 C 390 190, 410 190, 400 220 Z" fill="#cc0000" stroke="#cc0000" stroke-width="1"/>
  
  <!-- Vulnerabilities (radiating from brain) -->
  <!-- Identity Inference -->
  <circle cx="200" cy="170" r="60" fill="#ccccff" fill-opacity="0.7" stroke="#0066cc" stroke-width="2"/>
  <text x="200" y="160" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Identity</text>
  <text x="200" y="180" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Inference</text>
  <line x1="253" y1="207" x2="320" y2="255" stroke="#9966cc" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- Emotional State Mining -->
  <circle cx="200" cy="430" r="60" fill="#ccccff" fill-opacity="0.7" stroke="#0066cc" stroke-width="2"/>
  <text x="200" y="420" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Emotional</text>
  <text x="200" y="440" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">State</text>
  <text x="200" y="460" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Mining</text>
  <line x1="253" y1="393" x2="320" y2="345" stroke="#9966cc" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- Mental Health Profiling -->
  <circle cx="600" cy="170" r="60" fill="#ccccff" fill-opacity="0.7" stroke="#0066cc" stroke-width="2"/>
  <text x="600" y="155" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Mental</text>
  <text x="600" y="175" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Health</text>
  <text x="600" y="195" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Profiling</text>
  <line x1="547" y1="207" x2="480" y2="255" stroke="#9966cc" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- Preference Extraction -->
  <circle cx="600" cy="430" r="60" fill="#ccccff" fill-opacity="0.7" stroke="#0066cc" stroke-width="2"/>
  <text x="600" y="415" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Preference</text>
  <text x="600" y="435" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">& Behavior</text>
  <text x="600" y="455" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Extraction</text>
  <line x1="547" y1="393" x2="480" y2="345" stroke="#9966cc" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- Data Sources -->
  <rect x="350" y="120" width="100" height="40" rx="10" ry="10" fill="#ffcccc" stroke="#cc0000" stroke-width="2"/>
  <text x="400" y="145" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">EEG Data</text>
  <line x1="400" y1="160" x2="400" y2="180" stroke="#cc0000" stroke-width="2"/>
  
  <rect x="350" y="440" width="100" height="40" rx="10" ry="10" fill="#ffcccc" stroke="#cc0000" stroke-width="2"/>
  <text x="400" y="465" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">fMRI Data</text>
  <line x1="400" y1="440" x2="400" y2="420" stroke="#cc0000" stroke-width="2"/>
  
  <rect x="220" y="300" width="100" height="40" rx="10" ry="10" fill="#ffcccc" stroke="#cc0000" stroke-width="2"/>
  <text x="270" y="325" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">BCI Signals</text>
  <line x1="320" y1="320" x2="340" y2="320" stroke="#cc0000" stroke-width="2"/>
  
  <rect x="480" y="300" width="100" height="40" rx="10" ry="10" fill="#ffcccc" stroke="#cc0000" stroke-width="2"/>
  <text x="530" y="325" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Neural Implants</text>
  <line x1="480" y1="320" x2="460" y2="320" stroke="#cc0000" stroke-width="2"/>
  
  <!-- Protection Mechanisms -->
  <rect x="70" y="300" width="110" height="40" rx="10" ry="10" fill="#9966cc" fill-opacity="0.7" stroke="#9966cc" stroke-width="2"/>
  <text x="125" y="325" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Anonymization</text>
  <line x1="180" y1="300" x2="200" y2="230" stroke="#9966cc" stroke-width="1" stroke-dasharray="5,5"/>
  <line x1="180" y1="340" x2="200" y2="370" stroke="#9966cc" stroke-width="1" stroke-dasharray="5,5"/>
  
  <rect x="620" y="300" width="110" height="40" rx="10" ry="10" fill="#9966cc" fill-opacity="0.7" stroke="#9966cc" stroke-width="2"/>
  <text x="675" y="325" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle">Encryption</text>
  <line x1="620" y1="300" x2="600" y2="230" stroke="#9966cc" stroke-width="1" stroke-dasharray="5,5"/>
  <line x1="620" y1="340" x2="600" y2="370" stroke="#9966cc" stroke-width="1" stroke-dasharray="5,5"/>
  
  <!-- Central risk icon -->
  <path d="M 400 300 m -15, 0 a 15,15 0 1,0 30,0 a 15,15 0 1,0 -30,0" fill="#cc9900" stroke="black" stroke-width="2"/>
  <text x="400" y="305" font-family="Arial" font-size="18" font-weight="bold" text-anchor="middle">!</text>
  
  <!-- Legend -->
  <rect x="600" y="520" width="180" height="60" fill="#f9f9f9" stroke="#000000" stroke-width="1"/>
  <circle cx="615" y="535" r="5" fill="#cc0000"/>
  <text x="630" y="540" font-family="Arial" font-size="12" text-anchor="start">Neural Data Sources</text>
  <circle cx="615" y="555" r="5" fill="#0066cc"/>
  <text x="630" y="560" font-family="Arial" font-size="12" text-anchor="start">Privacy Vulnerabilities</text>
  <circle cx="615" y="575" r="5" fill="#9966cc"/>
  <text x="630" y="580" font-family="Arial" font-size="12" text-anchor="start">Protection Mechanisms</text>
  
  <!-- Figure Number and Caption -->
  <text x="400" y="580" font-family="Arial" font-size="12" text-anchor="middle">Figure 15.2: Neural data privacy vulnerabilities and protection methods in NeuroAI systems</text>
</svg>